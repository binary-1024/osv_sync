# 二进制文件使用 LFS
*.jar filter=lfs diff=lfs merge=lfs -text
*.zip filter=lfs diff=lfs merge=lfs -text
# 大型数据文件使用 LFS
*.csv filter=lfs diff=lfs merge=lfs -text
# 文本文件统一使用 LF 换行符
*.json text eol=lf
*.log text eol=lf
*.txt text eol=lf
*.md text eol=lf
*.yml text eol=lf
*.yaml text eol=lf
.gitignore text eol=lf
.gitattributes text eol=lf
LICENSE text eol=lf
*.py text eol=lf
*.sh text eol=lf
*.xml filter=lfs diff=lfs merge=lfs -text
